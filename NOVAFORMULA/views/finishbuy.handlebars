<h1>Detalhes da Compra</h1>
<div class="market-container">
  <table class="table table-hover table-bordered table-dark tb-color">
    <thead>
      <tr>
        <th scope="col">Jogo</th>
        <th scope="col">Preço</th>
        <th scope="col">Chave de Ativação</th>
      </tr>
    </thead>
    <tbody>
      {{#each jogo}}
        <tr>
          <td>{{jogosNome}}</td>
          <td>R${{PrecoJogo}}</td>
          <td>{{ChaveID}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  <form action="/delete/carrinho" method="POST">
    <div class="btn-group">
      <input type="hidden" name="id" value="{{this.jogosID}}" />
      <button class="btn btn-primary" name="delete" type="submit">Voltar ao
        início</button>
    </div>
  </form>
</div>